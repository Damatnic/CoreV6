{
  "name": "astral-core-v5",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "vercel:build": "npm run typecheck && npm run lint && npm run build",
    "deploy:check": "node scripts/deployment-check.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:security": "npm audit && npm run test:owasp",
    "test:owasp": "node scripts/security-audit.js",
    "test:a11y": "node scripts/accessibility-audit.js",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build -t astralcore:latest .",
    "docker:run": "docker run -p 3000:3000 astralcore:latest",
    "deploy:staging": "npm run build && npm run deploy:staging-push",
    "deploy:production": "npm run test:ci && npm run build && npm run deploy:prod-push",
    "deploy:staging-push": "node scripts/deploy-staging.js",
    "deploy:prod-push": "node scripts/deploy-production.js",
    "backup:create": "node scripts/backup.js create",
    "backup:restore": "node scripts/backup.js restore",
    "monitor:start": "node scripts/monitoring.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@socket.io/admin-ui": "^0.5.1",
    "@tanstack/react-query": "^5.86.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@use-gesture/react": "^10.3.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "bad-words": "^4.0.0",
    "bcryptjs": "^3.0.2",
    "canvas-confetti": "^1.9.3",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "next": "14.2.32",
    "next-auth": "^4.24.11",
    "postcss": "^8.5.6",
    "prisma": "^6.15.0",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^3.4.17",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^10.0.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.32",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "msw": "^2.11.1",
    "node-mocks-http": "^1.17.2",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5"
  }
}
